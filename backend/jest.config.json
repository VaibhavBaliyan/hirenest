{
  "testEnvironment": "node",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/uploads/",
    "/edgestore/",
    "seed-jobs.js"
  ],
  "testMatch": ["**/__tests__/**/*.test.js"],
  "collectCoverageFrom": [
    "controllers/**/*.js",
    "models/**/*.js",
    "middleware/**/*.js",
    "utils/**/*.js"
  ],
  "testTimeout": 15000,
  "verbose": true,
  "setupFilesAfterEnv": ["./__tests__/setup.js"]
}
